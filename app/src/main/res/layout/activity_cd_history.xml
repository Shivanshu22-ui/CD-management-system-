<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/black"
    android:layout_height="match_parent"
    tools:context=".cdHistory">
    <TextView
        android:layout_marginTop="10dp"
        android:layout_marginStart="10sp"
        android:id="@+id/list_name"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="20sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
    <TextView
        android:layout_marginStart="10dp"
        android:id="@+id/list_branchcode"
        android:textColor="@color/white"
        android:layout_below="@+id/list_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
    <TextView
        android:layout_marginStart="10dp"
        android:id="@+id/list_fund"
        android:textColor="@color/white"
        android:layout_below="@+id/list_branchcode"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />

    <TextView
        android:id="@+id/list_date"
        android:layout_width="match_parent"
        android:textColor="@color/white"
        android:layout_height="wrap_content"
        android:layout_below="@+id/list_fund"
        android:layout_marginStart="10dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:id="@+id/list_currentuser"
        android:layout_marginStart="10dp"
        android:layout_below="@id/list_date"
        android:layout_marginBottom="10dp"
        />

    <ListView
        android:id="@+id/listview2"
        android:layout_below="@+id/list_currentuser"
        android:textColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="10sp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10sp"
        android:divider="@null"
        android:dividerHeight="10dp" />
</RelativeLayout>